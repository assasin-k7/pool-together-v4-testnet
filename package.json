{
  "name": "v4-testnet",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:pooltogether/v4-rinkeby.git",
  "author": "Brendan Asselstine <mail.asselstine@gmail.com>",
  "license": "MIT",
  "scripts": {
    "addPrizes": "hardhat run ./scripts/addPrizes.js --network ",
    "clean": "rm -rf cache/ artifacts/",
    "compile": "hardhat compile",
    "task": "hardhat",
    "verify-mumbai": "hardhat --network mumbai etherscan-verify --api-key $POLYGONSCAN_API_KEY",
    "deploy-mumbai": "DEPLOY=mumbai hardhat deploy --network mumbai",
    "deploy-rinkeby": "DEPLOY=rinkeby hardhat deploy --network rinkeby",
    "deploy-goerli": "DEPLOY=goerli hardhat deploy --network goerli",
    "disburse": "hardhat run ./scripts/disburse.js --network ",
    "deposit": "hardhat run ./scripts/deposit.js --network ",
    "generate": "node ./scripts/generateContractList.js",
    "node-mumbai": "DEPLOY=mumbai hardhat node",
    "node-rinkeby": "DEPLOY=rinkeby hardhat node",
    "node-goerli": "DEPLOY=goerli hardhat node",
    "prepare-deploy": "yarn clean && yarn compile",
    "reinstall": "rm -rf node_modules/ && yarn cache clean && yarn"
  },
  "dependencies": {
    "@pooltogether/draw-calculator-js": "^1.0.0-beta.24",
    "@pooltogether/owner-manager-contracts": "1.1.0",
    "@pooltogether/v4-core": "github://pooltogether/v4-core",
    "@pooltogether/v4-periphery": "github://pooltogether/v4-periphery#master",
    "@pooltogether/v4-timelocks": "github://pooltogether/v4-timelocks#master",
    "@pooltogether/yield-source-interface": "^1.1.0",
    "find": "^0.3.0",
    "glob": "^7.1.7"
  },
  "devDependencies": {
    "ajv": "^6.12.2",
    "chalk": "^4.1.2",
    "colors": "^1.4.0",
    "debug": "^4.3.2",
    "ethers": "^5.4.6",
    "hardhat": "^2.6.3",
    "hardhat-dependency-compiler": "^1.1.1",
    "hardhat-deploy": "^0.9.1",
    "hardhat-deploy-ethers": "^0.3.0-beta.10",
    "hardhat-ethers": "^1.0.1",
    "luxon": "^2.0.2",
    "prompt": "^1.2.0"
  }
}
